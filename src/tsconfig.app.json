{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
